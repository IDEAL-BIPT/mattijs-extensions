FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

topAir
{
    dimensions      [ 1 -1 -2 0 0 0 0 ];

    internalField   uniform 100000;

    boundaryField
    {
        maxY
        {
            type            calculated;
            value           uniform 100000;
        }
        minX
        {
            type            calculated;
            value           uniform 100000;
        }
        maxX
        {
            type            calculated;
            value           uniform 100000;
        }
        minZ
        {
            type            calculated;
            value           uniform 100000;
        }
        maxZ
        {
            type            calculated;
            value           uniform 100000;
        }
        topAir_to_rightSolid
        {
            type            calculated;
            value           uniform 100000;
        }
        topAir_to_heater
        {
            type            calculated;
            value           uniform 100000;
        }
        topAir_to_leftSolid
        {
            type            calculated;
            value           uniform 100000;
        }
    }
}

